/* Media Query */
@media (max-width: 850px) {
  .header {
    flex-direction: row; }
  .header img {
    width: 200px; }
  h1 {
    width: 100%; }
  .grid-container {
    grid-template-columns: repeat(2, 1fr);
    grid-template-columns: repeat(2, 1fr);
    row-gap: 10px; }
  .main-nosotros img {
    width: 400px; } }

@media (max-width: 680px) {
  .header {
    flex-direction: column; } }

@media (max-width: 680px) {
  .header {
    flex-direction: column; }
