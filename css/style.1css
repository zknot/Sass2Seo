* {
  margin: 0;
  padding: 0;
  box-sizing: border-box; }

.header {
  display: flex;
  justify-content: space-between;
  align-items: center; }

.header .logoPrincipal {
  display: flex;
  flex-direction: column;
  margin: 0 auto; }

h1 {
  display: flex;
  flex-direction: column;
  align-items: center; }

.header {
  background-color: #7F055F; }
  .header .logoPrincipal {
    background-color: #7F055F;
    display: flex;
    flex-direction: column;
    margin: 0 auto; }
  .header .navbar {
    background-color: #7F055F; }
    .header .navbar .navbar-nav a {
      color: #FFE8D4;
      text-decoration: none;
      font-family: "Nunito", sans-serif; }
      .header .navbar .navbar-nav a:hover {
        color: #E5A4CB;
        font-weight: bold; }

h1 {
  font-family: 'Nunito';
  background-color: #E5A4CB;
  width: 100%;
  color: white;
  text-decoration: none; }
  h1:hover {
    color: #7F055F;
    transition: .3s; }

.main-index,
.main-nosotros,
.main-contacto,
.main-modelos,
.main-faq {
    display: flex;
    flex-direction: column;
    align-items: center;
    background-image: url(../assets/img/mimilusfondobody.png);
    background-repeat: 0;
    background-position: center;
}



.main-index, .main-nosotros, .main-contacto, .main-modelos, .ubicacionMapa {
    display: flex;
